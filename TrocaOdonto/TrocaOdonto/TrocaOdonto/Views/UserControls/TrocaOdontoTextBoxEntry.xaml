<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TrocaOdonto.Views.UserControls.TrocaOdontoTextBoxEntry">
  <ContentView.Content>
        <Frame>
            <StackLayout Orientation="Horizontal">
                <Entry x:Name="TextFieldName" PlaceholderColor="{StaticResource SecundaryTextColor}" 
                       TextColor="{StaticResource SecundaryTextColor}"></Entry>
                <Image x:Name="ImageIcon" WidthRequest="30" HeightRequest="30"
                       Margin="0, 0, 10, 0"
                       HorizontalOptions="EndAndExpand"></Image>
            </StackLayout>
        </Frame>
    </ContentView.Content>
</ContentView>